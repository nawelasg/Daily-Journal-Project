<%- include ("partials/header.ejs"); -%>

<h1>Home</h1>

<p> <%= startingContent %> </p>

<% posts.forEach(function(post) { %>
    <% console.log(post.title) %>

    <h1><%= post.title %></h1>

    <p><%= post.content.substring(0,100) + "..." %></p>
    <a href="/posts/<%= post.title%>">Read More</a>
<% }) %>

<%- include ("partials/footer.ejs"); -%>